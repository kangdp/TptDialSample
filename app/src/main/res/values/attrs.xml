<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TptDialView">
        <!--刻度盘内圆半径-->
        <attr name="InnerRadius" format="dimension"/>
        <!--刻度线高度-->
        <attr name="TickMarkHeight" format="dimension"/>
        <!--刻度线渐变的起始颜色-->
        <attr name="TickMarkStartColor" format="color|reference"/>
        <!--刻度线渐变的结束颜色-->
        <attr name="TickMarkEndColor" format="color|reference"/>
        <!--选中的刻度线高度-->
        <attr name="SelTickMarkHeight" format="dimension"/>
        <!--每个刻度的角度-->
        <attr name="TickMarkAngle" format="float"/>
        <!--刻度之间的间隔角度-->
        <attr name="TickMarkSpaceAngle" format="integer"/>
        <!--刻度盘的起始角度-->
        <attr name="TickMarkStartAngle" format="integer"/>
        <!--所有刻度扫过的角度-->
        <attr name="TickMarkSweepAngle" format="integer"/>
        <!--当前刻度指针的位置-->
        <attr name="CurPosition" format="integer"/>
        <attr name="Thumb" format="reference"/>
    </declare-styleable>



</resources>