<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TptDialView">
        <!--刻度盘内圆半径-->
        <attr name="InnerRadius" format="dimension"/>
        <!--刻度线高度-->
        <attr name="TickMarkHeight" format="dimension"/>
        <!--刻度线渐变的起始颜色-->
        <attr name="TickMarkStartColor" format="color"/>
        <!--刻度线渐变的结束颜色-->
        <attr name="TickMarkEndColor" format="color"/>
        <!--选中的刻度线高度-->
        <attr name="SelTickMarkHeight" format="dimension"/>
        <!--每个刻度的角度-->
        <attr name="TickMarkAngle" format="float"/>
        <!--刻度之间的间隔角度-->
        <attr name="TickMarkSpaceAngle" format="integer"/>
        <!--刻度盘的起始角度-->
        <attr name="TickMarkStartAngle" format="integer"/>
        <!--所有刻度扫过的角度-->
        <attr name="TickMarkSweepAngle" format="integer"/>
        <!--当前刻度指针的位置-->
        <attr name="CurPosition" format="integer"/>
        <!--指针上的thumb-->
        <attr name="ThumbRadius" format="dimension"/>
        <attr name="ThumbColor" format="color"/>
        <attr name="ThumbStrokeWidth" format="dimension"/>
        <attr name="ThumbStrokeColor" format="color"/>
        <attr name="ThumbShadowColor" format="color"/>
        <attr name="ThumbShadowRadius" format="dimension"/>
        <!--虚线颜色-->
        <attr name="DottedColor" format="color"/>
        <!--最小数值-->
        <attr name="MinValue" format="float"/>
        <!--最大数值-->
        <attr name="MaxValue" format="float"/>
    </declare-styleable>




</resources>